
<div class="modal centered-modal" id="editVenue" tabindex="-1" role="dialog" aria-labelledby="" aria-hidden="true" data-backdrop="static" data-keyboard="false">
  <div class="modal-dialog centered-modal">
    <div class="modal-content">

        <div class="modal-header">
          <button type="button" class="close" data-dismiss="modal" aria-hidden="true" ng-if="!venueLoading">&times;</button>
          <h4 class="modal-title" id="">Edit venue</h4>
        </div>
      <div class="modal-body">
        <div class="row">
          <!-- Venue -->
          <div class="col-md-12">
            <div class="form-group">
              <div class="input-group">
                <span class="input-group-addon" id="sizing-addon2"><span class="glyphicon glyphicon-flag"></span></span>
                <input ng-model="venue" type="text" class="form-control" aria-describedby="sizing-addon2" placeholder="Add a venue"/>
              </div>
            </div>
          </div>
        </div>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-default" data-dismiss="modal" ng-if="!venueLoading">Close</button>
        <button type="button" class="btn btn-primary" ladda="venueLoading" data-style="expand-right" data-spinner-size="10" ng-click="EditVenue(venue)">Update</button>
      </div>
    </div>
  </div>
</div>
